{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "cypress/globals": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "strict": 1
    },
    "plugins": [
        "cypress"
    ]
}