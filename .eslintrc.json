{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "strict": 1
    },
    "globals": {
        "Cypress": true,
        "cy": true,
        "describe": true,
        "context": true,
        "before": true,
        "beforeEach": true,
        "after": true,
        "afterEach": true,
        "it": true,
        "expect": true
    }
}